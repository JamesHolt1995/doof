<style lang="scss">
	.text {
		position: absolute;
		z-index: 10;
		top: 50%;
		left: 0;
		width: 100%;
		display: flex;
		justify-content: space-between;
		font-family: var(--dot-font);
		font-weight: 100;
		font-size: 30px;
		color: var(--grey);
		padding: 0 10px;
		p {
			text-align: justify;
			margin: 0;
		}
	}
	.svg-container {
		position: absolute;
		padding: 10px;
		z-index: 10;
		left: 0;
		top: 0px;
		width: 100%;
	}
	@media screen and (min-width: 900px) {
		.svg-container {
			top: 50px;
			padding: 20px;
		}
		.text {
			padding: 20px;
		}
	}
	svg {
		width: 100%;
	}
	video {
		width: 100%;
		height: 100%;
		min-width: 100vw;
		min-height: 100vh;
		object-fit: cover;
		pointer-events: none;
	}
</style>
